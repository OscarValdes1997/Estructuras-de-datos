#includide <stdlib.h>
//L a complegidad del programa es 0(n)

int n=0;

int FacRecurcion(int n){
if(n==1 || n==0){
  return 1;
  }else{
    n = n*FacRecurcion(n-1);
    printf("Num: %i\n", n);
    return n;
    
    }
}

